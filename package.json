{
  "name": "marketplace_client",
  "version": "0.0.4",
  "description": "Decentralized Marketplace Client (PoC)",
  "scripts": {
    "clean": "rm -rf ./dist/*/*/latest.json ./dist/*/*/blockchain/public ./dist/*/*/blockchain/private ./dist/*/client-* ./dist/*/*/client-* ./build",
    "postinstall": "npm run clean && ./scripts/postinstall.sh; exit $?",
    "test": "echo \"Error: no test specified\" && exit 1",
    "alice": "(cd ./dist/$(uname -s | awk '{ print tolower($0) }')/alice; cat ./config.toml | ./../../../node_modules/.bin/coffee ./../../../client.coffee; cd -)",
    "bob": "(cd ./dist/$(uname -s | awk '{ print tolower($0) }')/bob; cat ./config.toml | ./../../../node_modules/.bin/coffee ./../../../client.coffee; cd -)",
    "james": "(cd ./dist/$(uname -s | awk '{ print tolower($0) }')/james; cat ./config.toml | ./../../../node_modules/.bin/coffee ./../../../client.coffee; cd -)",
    "tom": "(cd ./dist/$(uname -s | awk '{ print tolower($0) }')/tom; cat ./config.toml | ./../../../node_modules/.bin/coffee ./../../../client.coffee; cd -)",
    "cl-1": "(cd ./dist/$(uname -s | awk '{ print tolower($0) }')/cl-1; cat ./config.toml | ./../../../node_modules/.bin/coffee ./../../../client.coffee; cd -)",
    "release": "npm run clean && ./scripts/release.sh; if [ $? -ne 0 ]; then echo 'failed!!!'; exit 9; fi; echo 'client released'"
  },
  "keywords": [
    "decentralized",
    "libp2p",
    "marketplace",
    "p2p",
    "PoC"
  ],
  "author": "Vi+ <vitaliyy@gmail.com>",
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "file-type": "^7.6.0",
    "leveldown": "^3.0.0",
    "levelup": "^2.0.2",
    "lowdb": "^1.0.0",
    "opn": "^5.2.0",
    "read-chunk": "^2.1.0",
    "toml": "^2.3.3",
    "libp2p": "^0.18.0",
    "libp2p-kad-dht": "^0.8.0",
    "libp2p-spdy": "^0.11.0",
    "libp2p-tcp": "^0.11.6",
    "libp2p-railing": "^0.7.1",
    "libp2p-secio": "^0.9.3",
    "libp2p-webrtc-star": "^0.13.4",
    "libp2p-websocket-star": "^0.7.7",
    "libp2p-multiplex": "^0.5.1",
    "libp2p-mplex": "^0.6.0",
    "libp2p-floodsub": "^0.14.1",
    "libp2p-mdns": "^0.9.2",
    "libp2p-websockets": "^0.10.5",
    "libp2p-crypto": "^0.12.1",
    "peer-id": "^0.10.6",
    "peer-info": "^0.11.6",
    "pull-pushable": "^2.2.0",
    "pull-stream": "^3.6.2"
  },
  "devDependencies": {
    "coffeescript": "^2.2.2",
    "js2coffee": "^2.2.0",
    "pkg": "^4.3.0",
    "json": "^9.0.6"
  },
  "bin": "client.js",
  "engines": {
    "node": ">=8.8.0",
    "npm": ">=5.6.0"
  }
}
